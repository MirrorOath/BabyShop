<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="dao.tables" auto-import="true">
	<class name="CommodityInfo" >
		<!-- 主键 ，映射-->
		<id name="id" column="id">
			 <generator class="increment"/>
		</id>
		<property name="category" column="category" type="string" />
		<property name="name" column="name" type="string" />
        <property name="price" column="price" />
		<property name="imageSrc" column="imageSrc" type="string" />
        <property name="note" column="note" type="string" />
	</class>
	
</hibernate-mapping>
